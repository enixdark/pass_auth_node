<% if (paginate.hasPreviousPages() || paginate.hasNextPages(pageCount)) { %>
  <div class="navigation. well-sm" id="pagination">
    <ul class="pager">
      <% if (paginate.hasPreviousPages) { %>
        <li class="previous">
          <a href=<%= paginate.href(true) %> class="prev" %>
            <div class="i fa fa-arrow-circle-left">
            <p>Previous</p>d
            </div>
          </a>
        </li>
      <% } %>

      <% if (pages) { %>
        <% for(let page in pages) { %>
          <a class="btn btn-default" href=<%= page.url %> > <%= page.number %> </a>
        <% } % %>
      <% } %>

      <% if(paginate.hasNextPages(pageCount)) { %>
        <li class="next" %>
          <a href=<%=paginate.href()).next %> 
            <p>Next&nbsp;</p>
            <div class="i fa fa-arrow-circle-right"></div>
          </a>
        </li>
      <% } %>

    </ul>
  </div>
<% } %>